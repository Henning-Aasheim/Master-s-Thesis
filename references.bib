	keywords = {read},
	pages = {133--147},
	annote = {This article examines whether citizens in the US disagree on political facts because of partisan cheerleading or a genuine difference of opinion. the researchers find evidence for the latter, especially by looking into if incentives would change answers and which media sources the respondents utilised. they also looked at this against normal web browsing. There was a small cheerleading effect, but the main explanation was found in motivated reasoning, i.e., genuine difference of opinion. 
},
	file = {Full Text PDF:C\:\\Users\\ny bruker\\Zotero\\storage\\6YWRVG5H\\Peterson og Iyengar - 2021 - Partisan Gaps in Political Information and Informa.pdf:application/pdf},
