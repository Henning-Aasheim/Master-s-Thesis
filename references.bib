	keywords = {read},
	pages = {824--839},
	file = {Full Text PDF:C\:\\Users\\ny bruker\\Zotero\\storage\\IETSNWYY\\Bisgaard - 2019 - How Getting the Facts Right Can Fuel Partisan-Moti.pdf:application/pdf},
}

@article{clayton_elite_2021,
