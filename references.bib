@article{collier_conceptual_1993,
	title = {Conceptual "{Stretching}" {Revisited}: {Adapting} {Categories} in {Comparative} {Analysis}},
	volume = {87},
	issn = {0003-0554},
	shorttitle = {Conceptual "{Stretching}" {Revisited}},
	url = {https://www.jstor.org/stable/2938818},
	doi = {10.2307/2938818},
	abstract = {When scholars extend their models and hypotheses to encompass additional cases, they commonly need to adapt their analytic categories to fit the new contexts. Giovanni Sartori's work on conceptual "traveling" and conceptual "stretching" provides helpful guidance in addressing this fundamental task of comparative analysis. Yet Sartori's framework draws upon what may be called classical categorization, which views the relation among categories in terms of a taxonomic hierarchy, with each category having clear boundaries and defining properties shared by all members. We examine the challenge to this framework presented by two types of nonclassical categories: family resemblances and radial categories. With such categories, the overly strict application of a classical framework can lead to abandoning to category prematurely or to modifying it inappropriately. We discuss solutions to these problems, using examples of how scholars have adapted their categories in comparative research on democracy and authoritarianism.},
	number = {4},
	urldate = {2024-12-05},
	journal = {The American Political Science Review},
	author = {Collier, David and Mahon, James E.},
	month = dec,
	year = {1993},
	note = {Publisher: [American Political Science Association, Cambridge University Press]},
	keywords = {read},
	pages = {845--855},
	annote = {Collier and Mahon faces the problem of concept formation. Specifically they critique Sartori’s ladder of ‘generality’ as sometimes being too strict.
When defining a concept, there is a trade-pff between extension and intension, where, if you want to extend the cases a concept/category covers, the law of inverse variation says that the intention, or meaning, of the concept must be limited to some degree, i.e., it becomes a more general concept/category.
Collier and Mahon also discusses three types of categories: classical, family resemblance, and radial.
The classical category is defined by a superordinate primary category, where adding attributes creates secondary categories that are more specific and, thus, has less intention. The reverse is true for radial categories, where the primary category denotes a high-intention ‘true’ category, and the secondary categories are more realistic categories with greater extent, as they loosen the intention criteria. The family resemblance category is when cases have many similar attributes that together make them a group, but most do not include all the attributes. These are possible, but under the rules of the ladder of generality, making a category to include every case, would remove all the attributes and make the category non-differentiated. 
They argue that Sartori’s ladder mainly works for the classical type of concept/category, while, especially, family resemblance needs looser criteria. 
“The goal of this discussion has been to offer new guidelines for comparative analysts who are concerned with the problems of conceptual traveling and conceptual stretching.” (Collier and Mahon, 1993, p. 852)
“overly strict application of classical principles of categorization can lead to the premature abandonment of potentially useful categories.” (Collier and Mahon, 1993, p. 852)

},
	file = {JSTOR Full Text PDF:C\:\\Users\\ny bruker\\Zotero\\storage\\VHLCULGZ\\Collier and Mahon - 1993 - Conceptual Stretching Revisited Adapting Catego.pdf:application/pdf},
}

@article{gerring_what_1999,
	title = {What {Makes} a {Concept} {Good}? {A} {Criterial} {Framework} for {Understanding} {Concept} {Formation} in the {Social} {Sciences}},
	volume = {31},
	issn = {0032-3497},
	shorttitle = {What {Makes} a {Concept} {Good}?},
	url = {https://www.jstor.org/stable/3235246},
	doi = {10.2307/3235246},
	abstract = {Nowhere in the broad and heterogeneous work on concept formation has the question of conceptual utility been satisfactorily addressed. Goodness in concept formation, I argue, cannot be reduced to 'clarity,' to empirical or theoretical relevance, to a set of rules, or to the methodology particular to a given study. Rather, I argue that conceptual adequacy should be perceived as an attempt to respond to a standard set of criteria, whose demands are felt in the formation and use of all social science concepts: (1) familiarity, (2) resonance, (3) parsimony, (4) coherence, (5) differentiation, (6) depth, (7) theoretical utility, and (8) field utility. The significance of this study is to be found not simply in answering this important question, but also in providing a complete and reasonably concise framework for explaining the process of concept formation within the social sciences. Rather than conceiving of concept formation as a method (with a fixed set of rules and a definite outcome), I view it as a highly variable process involving trade-offs among these eight demands.},
	number = {3},
	urldate = {2024-12-05},
	journal = {Polity},
	author = {Gerring, John},
	year = {1999},
	note = {Publisher: Palgrave Macmillan Journals},
	keywords = {read},
	pages = {357--393},
	annote = {Gerring here proposes that concept formation is a process of trade-offs between eight demands: familiarity, resonance, parsimony, coherence, differentiation, depth, theoretical utility, and field utility (Gerring, 367). 
},
	file = {JSTOR Full Text PDF:C\:\\Users\\ny bruker\\Zotero\\storage\\GMYUWURS\\Gerring - 1999 - What Makes a Concept Good A Criterial Framework f.pdf:application/pdf},
}

@article{maggetti_problems_2016,
	title = {Problems (and solutions) in the measurement of policy diffusion mechanisms},
