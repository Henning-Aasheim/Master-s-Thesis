@article{bermeo_democratic_2016,
	title = {On {Democratic} {Backsliding}},
	volume = {27},
	issn = {1086-3214},
	url = {https://muse.jhu.edu/pub/1/article/607612},
	abstract = {Democratic backsliding (meaning the state-led debilitation or elimination of the political institutions sustaining an existing democracy) has changed dramatically since the Cold War. Open-ended coups d’état, executive coups, and blatant election-day vote fraud are declining while promissory coups, executive aggrandizement and strategic electoral manipulation and harassment are increasing. Contemporary forms of backsliding are especially vexing because they are legitimated by the very institutions democracy promoters prioritize but, overall, backsliding today reflects democracy’s advance and not its retreat. The current mix of backsliding is more easily reversible than the past mix and successor dictatorships are shorter-lived and less authoritarian.},
	number = {1},
	urldate = {2023-12-26},
	journal = {Journal of Democracy},
	author = {Bermeo, Nancy},
	month = jan,
	year = {2016},
	note = {Publisher: Johns Hopkins University Press},
	keywords = {read},
	pages = {5--19},
	annote = {This paper looks at 6 ways of democratic backsliding, 3 of which has been on the decline; coups d’état, executive coups, and election-day vote fraud. 3 others have been on the rise; promissory coups, executive aggrandisement, and election manipulation strategies. this has lead to more incremental rather than sudden backsliding, which is good since it is less violent and leaves room for correction, but it is harder to notice.
},
	file = {Full Text PDF:C\:\\Users\\ny bruker\\Zotero\\storage\\GH3UIWC8\\Bermeo - 2016 - On Democratic Backsliding.pdf:application/pdf},
}

